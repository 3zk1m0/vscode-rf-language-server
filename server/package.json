  "author": {
    "name": "Tomi Turtiainen",
    "email": "tomi.turtiainen@gmail.com"
  },
  "engines": {
    "node": "*"
  },
  "dependencies": {
    "lodash": "4.17.4",
    "vscode-languageserver": "2.6.2",
    "vscode-uri": "1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/lodash": "^4.14.46",
    "@types/mocha": "^2.2.35",
    "@types/node": "^6.0.52",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "ts-node": "^2.0.0",
    "tslint": "^4.3.1",
    "typescript": "^2.1.4"
  },
  "main": "index.js",
  "scripts": {
    "compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
    "watch": "npm run clean && mkdir -p ../client/server/node_modules && cp -r node_modules/ ../client/server/node_modules && installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc --watch -p .",
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register **/*.test.ts",
    "clean": "rm -rf ../client/server",
    "lint": "tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tomi/vscode-rf-language-server.git"
  },
  "keywords": [
    "RF",
    "RobotFramework",
    "Intellisense"
  ],
  "bugs": {
    "url": "https://github.com/tomi/vscode-rf-language-server/issues"
  },
